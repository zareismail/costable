{
    "name": "zareismail/costable",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "optimistdigital/nova-input-filter": "^1.0",
        "zareismail/nova-cascade": "~0.1",
        "zareismail/nova-currency": "^0.1",
        "nemrutco/nova-global-filter": "~1.1",
        "zareismail/glance": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Zareismail\\Costable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zareismail\\Costable\\CostableServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
